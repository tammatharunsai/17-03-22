s=input()
l=list(s)
low=0
u = 0
d = 0
s = 0
for i in l:
    if (i.islower()):
     low = low+1 
    elif (i.isupper()):
        u = u+1 
    elif (i.isdigit()):
        d = d+1 
    else:
        s = s+1 
if(low>0 and u >0 and d >0 and s>0):
    print("The given passward is Valid")
else:
    print("Invalid passward")
